{
  "name": "mijn-b2b-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "workspaces": [
    "packages/*",
    "workers/*"
  ],
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:frontend": "run-p type-check \"build-only {@}\" --",
    "preview": "npm run build:frontend && wrangler dev",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "deploy:frontend": "wrangler pages deploy dist",
    "test": "vitest",
    "cf-typegen": "wrangler types"
  },
  "dependencies": {
    "@b2b/db": "workspace:*",
    "@aws-sdk/client-s3": "^3.879.0",
    "@aws-sdk/s3-request-presigner": "^3.879.0",
    "@heroicons/vue": "^2.2.0",
    "@tailwindcss/vite": "^4.1.12",
    "@vueuse/core": "^13.9.0",
    "pinia": "^3.0.3",
    "vue": "^3.5.22",
    "vue-i18n": "^9.14.5",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "^1.13.15",
    "@eslint/js": "^9.39.2",
    "@tsconfig/node22": "^22.0.2",
    "@types/node": "^22.18.6",
    "@typescript-eslint/eslint-plugin": "^8.53.1",
    "@typescript-eslint/parser": "^8.53.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.8.1",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-vue": "^10.7.0",
    "globals": "^17.0.0",
    "npm-run-all2": "^8.0.4",
    "prettier": "^3.8.1",
    "tailwindcss": "^4.1.12",
    "typescript": "^5.9.0",
    "typescript-eslint": "^8.53.1",
    "vite": "^7.1.7",
    "vite-plugin-vue-devtools": "^8.0.2",
    "vue-eslint-parser": "^10.2.0",
    "vue-tsc": "^3.1.0",
    "wrangler": "^4.45.0"
  }
}
