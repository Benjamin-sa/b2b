{
  "name": "mijn-b2b-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "workspaces": [
    "workers/*"
  ],
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "turbo run build",
    "build:frontend": "run-p type-check \"build-only {@}\" --",
    "preview": "npm run build:frontend && wrangler dev",
    "build-only": "vite build",
    "type-check": "turbo run type-check",
    "deploy": "turbo run deploy",
    "deploy:dev": "turbo run deploy:dev",
    "deploy:prod": "turbo run deploy:prod",
    "test": "turbo run test",
    "cf-typegen": "wrangler types",
    "deploy:dev:all": "turbo run deploy:dev"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.879.0",
    "@aws-sdk/s3-request-presigner": "^3.879.0",
    "@heroicons/vue": "^2.2.0",
    "@tailwindcss/vite": "^4.1.12",
    "@vueuse/core": "^13.9.0",
    "pinia": "^3.0.3",
    "vue": "^3.5.22",
    "vue-i18n": "^9.14.5",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "^1.13.15",
    "@tsconfig/node22": "^22.0.2",
    "@types/node": "^22.18.6",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.8.1",
    "npm-run-all2": "^8.0.4",
    "tailwindcss": "^4.1.12",
    "turbo": "^2.7.5",
    "typescript": "^5.9.0",
    "vite": "^7.1.7",
    "vite-plugin-vue-devtools": "^8.0.2",
    "vue-tsc": "^3.1.0",
    "wrangler": "^4.45.0"
  }
}
